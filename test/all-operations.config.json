{
  "$schema": "../ng-openapi-gen-schema.json",
  "input": "all-operations.json",
  "output": "out/all-operations",
  "defaultTag": "noTag",
  "indexFile": true,
  "apiService": false,
  "excludeParameters": [
    "X-Exclude"
  ],
  "keepFullResponseMediaType": [
    {
      "mediaType": "spring",
      "use": "full"
    },
    {
      "mediaType": "hal\\+json",
      "use": "tail"
    }
  ]
}
